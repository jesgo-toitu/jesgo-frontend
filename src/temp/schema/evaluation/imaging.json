{
    "$schema": "../jesgo.json",
    "$id": "/schema/evaluations/imaging",
    "type": "object",
    "title": "画像",
    "properties": {
        "検査実施日": {
            "type": "string",
            "format": "date",
            "jesgo:set": "eventdate"
        },
        "モダリティ": {
            "type": "string",
            "enum": [
                "CT",
                "造影CT",
                "MRI",
                "造影MRI",
                "ダイナミックMRI",
                "PET",
                "PET-CT",
                "骨シンチグラフィ",
                "超音波断層法"
            ]
        },
        "所見": {
            "type": "string"
        }
    }
}
