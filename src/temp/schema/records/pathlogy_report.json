{
    "$schema": "../jesgo.json",
    "$id": "/schema/records/pathlogy_report",
    "type": "object",
    "title": "病理診断レポート",
    "jesgo:parentschema": [
        "/schema/*/pathology"
    ],
    "jesgo:ui:subschemastyle": "column",
    "properties": {
        "検査実施日": {
            "type": "string",
            "format": "date",
            "jesgo:set": "eventdate"
        },
        "標本番号": {
            "type": "string"
        },
        "病理診断": {
            "type": "string",
            "jesgo:ui:textarea": true
        },
        "所見": {
            "type": "string",
            "jesgo:ui:textarea": true
        }
    }
}
